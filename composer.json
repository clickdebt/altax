{
  "name": "kohkimakimoto/altax",
  "description": "Altax is a simple deployment tool running SSH in parallel.",
  "keywords": ["deploy", "deployment", "ssh"],
  "homepage": "https://github.com/kohkimakimoto/altax",
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "Kohki Makimoto",
      "email": "kohki.makimoto@gmail.com",
      "homepage": "http://kohkimakimoto.hatenablog.com/"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "autoload": {
    "psr-0": { "Altax_": "src/" }
  },
  "include-path": ["src"],
  "extra": {
    "branch-alias": {
      "dev-master": "devel-composer"
    }
  },
  "bin": ["bin/altax"]
}
